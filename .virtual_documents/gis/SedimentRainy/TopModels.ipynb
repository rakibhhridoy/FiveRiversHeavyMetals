# Helper function to parse the new data format
def parse_data_new_format(data_str):
    lines = data_str.strip().split('\n')
    parsed_data = []
    for line in lines:
        if line.strip() == "":
            continue
        parts = line.rsplit(':', 1)
        feature = parts[0].strip()
        weight = float(parts[1].strip())
        parsed_data.append([feature, weight])
    return parsed_data

