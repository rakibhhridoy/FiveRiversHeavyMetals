def parse_lime_data(data_str):
    lines = data_str.strip().split('\n')
    parsed_data = []
    for line in lines:
        parts = line.split('|')
        feature = parts[0].replace('Feature:', '').strip()
        weight = float(parts[1].replace('Weight:', '').strip())
        parsed_data.append([feature, weight])
    return parsed_data

